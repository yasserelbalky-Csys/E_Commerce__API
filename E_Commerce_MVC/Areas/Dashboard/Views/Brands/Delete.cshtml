@model E_Commerce_MVC.Models.EntitiesViewModel.BrandViewModel
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Dashboard/Views/Shared/_Layout.cshtml";
}
<h1>Delete</h1>
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>BrandViewModel</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BrandName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BrandName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BrandDescription)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BrandDescription)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.b_deleted)
        </dt>
        <dd class="col-sm-10">
            <input type="checkbox" disabled="disabled" @(Model.b_deleted ? "checked" : "") />
        </dd>
    </dl>
    <form asp-action="DeleteConfirmed">
        <input type="hidden" name="id" value="@Model.BrandId" />
        <input type="hidden" asp-for="b_deleted" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>